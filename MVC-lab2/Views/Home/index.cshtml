@model Employee
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>index</title>
</head>
<body>
    <div> 
        <form method="post">
            Name:@Html.TextBoxFor(e => e.Name) @Html.ValidationMessageFor(e => e.Name)
            Email:@Html.TextBoxFor(e => e.Email) @Html.ValidationMessageFor(e => e.Email)
            Password: @Html.TextBoxFor(e => e.Password) @Html.ValidationMessageFor(e => e.Password)
            Address:@Html.TextAreaFor(e => e.Address) @Html.ValidationMessageFor(e => e.Address)

            <button type="submit">Add</button>
           








        </form>
    </div>
</body>
</html>
